		START	101


		READ	N
		MOVER	BREG ,	ONE
		MOVEM	BREG ,	TERM
AGAIN	MULT	BREG ,	TERM
		MOVER	CREG ,	TERM
		ADD	CREG ,	ONE
		MOVEM	CREG ,	TERM
		COMP	CREG ,	N
		BC	LE ,	AGAIN
		SECOND  23 ,    32 ,  CREG
		MOVEM	BREG ,	RESULT

		FIRST	34 ,	12 ,	BREG
		PRINT	RESULT
		STOP
		N	DS	1
		RESULT	DS	1
		ONE	DC	'1'
		TERM	DS	1
		END
